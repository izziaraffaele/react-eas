{
  "name": "react-eas",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "description": "A set of hooks to easily develop dApps using Ethereum Attestation Service and React",
  "license": "MIT",
  "repository": "https://github.com/izziaraffaele/react-eas.git",
  "author": "Izzia Raffaele <izziaraffaele@gmail.com>",
  "keywords": [
    "eas",
    "react",
    "wagmi",
    "ethers"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "treeshake": true,
    "sourcemap": "inline",
    "minify": true,
    "clean": true,
    "dts": true,
    "splitting": false,
    "format": [
      "cjs",
      "esm"
    ],
    "external": [
      "react"
    ],
    "injectStyle": false
  },
  "devDependencies": {
    "@types/node": "^20.4.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.30.1",
    "ethers": "^6.7.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "dependencies": {
    "@ethereum-attestation-service/eas-sdk": "^1.0.0-beta.0"
  }
}
